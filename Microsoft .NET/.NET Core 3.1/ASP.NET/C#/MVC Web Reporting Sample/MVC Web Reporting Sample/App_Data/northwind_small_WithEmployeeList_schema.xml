              <xs:element name="UnitPrice" type="xs:decimal" minOccurs="0" />
              <xs:element name="Quantity" type="xs:short" minOccurs="0" />
              <xs:element name="Discount" type="xs:float" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
